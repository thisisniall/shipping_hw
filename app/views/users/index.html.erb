<h2>All Users:</h2>
	<% @users.each do |u| %>
		<div class = 'userinfo'>
			<ul>
			<strong>Username:</strong> <%= link_to u.email, u %>
			<li><strong>First Name:</strong> <%= u.fname %></li>
			<li><strong>Last Name:</strong> <%= u.lname %></li>
			<li><strong>Boats:</strong> <%= "Some Boats Code Here"%></li>
			</ul>
		</div>
	<%end%>
</ul>
